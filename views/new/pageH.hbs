<div class="topo">
  <div class="row ">
    <div class="col rowclass">
      <img class="logo" src="/images/logo.png">
    </div>
    <div class="col-6 rowclass">
      <div class="acompanhantes">
        <label for="exampleInputPassword1"
          style="font-weight: 900; font-size: large; display:flex; justify-content: center;">Responsável:</label>
        <input type="text" class="form-control" value="{{client.fullname}}" readonly><br>
        <label for="exampleInputPassword1"
          style="font-weight: 900; font-size: large; display:flex; justify-content: center;">Acompanhantes:</label>
        {{#compare budget.finalized "Em andamento"}}
        <br><br>
        {{#each test}}
        <input type="text" class="form-control" value="{{name}}" readonly>
        <br>
        {{/each}}
        {{else}}
        {{#each budgetCompanionsToPage}}
        <input type="text" class="form-control" value="{{name}}" readonly>
        <br>
        {{/each}}
        {{/compare}}
      </div>
    </div>
    <div class="col rowclass info1_flex">
      <input type="text" value="Av. do Contorno, 7218" style="text-align:center;" readonly></input>
      <input type="text" value="30110-048 - Belo Horizonte" style="text-align:center;" readonly></input>
      <input type="text" value="www.anistravel.com.br" style="text-align: center;" readonly></input>
      <input type="text" value="@anis.travel" style="text-align: center;" readonly></input>
      <input type="text" value="(31)3080-7000" style="text-align: center;" readonly></input>
      <br><br><br><br>
    </div>
  </div>
</div>

