                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th colspan="8"><input name="safe[otherINF]" placeholder="Valor por bebê" type="text" id="inf{{repNum}}valOther" onblur="calcOther()" class="form-control" value="{{otherINF}}"></th>
                      <th colspan="1"><input name="safe[OthernumINF]" placeholder="Número de bebês" type="text" id="inf{{repNum}}multOther" onblur="calcOther()" class="form-control form-control--width autorow" value="{{OthernumINF}}"></th>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>

            <div class="rowclass">
              <table class="autorow">
                <thead>
                  <tr>
                    <th><label class="form-control--width autorow">Qual Moeda Utilizada:</label></th>
                    <th><label class="form-control--width autorow" for="exampleInputPassword1">Valor total:</label></th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <th>
                      <select placeholder="Selecione" class="form-control form-control--width" name=safe[otherCoin]>
