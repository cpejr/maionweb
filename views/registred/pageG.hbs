                      <th colspan="4"><input name="safe[otherCHD]" placeholder="Valor por criança" type="text" id="chd{{repNum}}valOther" onblur="calcOther()" class="form-control" value="{{otherCHD}}"></th>
                      <th colspan="1"><input name="safe[OthernumCHD]" placeholder="Número de crianças" type="text" id="chd{{repNum}}multOther" onblur="calcOther()" class="form-control form-control--width autorow" value="{{OthernumCHD}}"></th>
                    </tr>
                  </tbody>
                </table>
              </div>

              <div class="form-group">
                <table class="autorow">
                  <thead>
                    <tr>
