<script>
  function Mudarestado(divid){
    document.getElementById(divid).style.display = "none";

  }
    function docCsv(){
        var table = document.getElementById("mytable2").innerHTML;
        var data = table.replace(/<thead>/g, '')
            .replace(/<\/thead>/g, '')
            .replace(/<tbody id="client_table">/g, '')
            .replace(/<\/tbody>/g, '')
            .replace(/<tr>/g, '')
            .replace(/<\/tr>/g, '\r\n')
            .replace(/<th>/g, '')
            .replace(/<\/th>/g, ';')
            .replace(/\t/g, '')
            .replace(/\n/g, '');
        var mylink = document.createElement('a');
        mylink.download = "csvname.csv";
        mylink.href = "data:application/csv," + escape(data);
        mylink.click();
    }
</script>

